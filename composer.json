{
    "name": "ricovandevin/patternlab-twig-extensions-plugin",
    "description": "Adds the ability to load custom Twig extensions into PatternLab.",
    "type": "patternlab-plugin",
    "require": {
        "vanpattenmedia/twig-pluralize-extension": "^1.0",
        "composer/installers": "^1.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rico van de Vin",
            "email": "rico@finlet.eu"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "installer-paths": {
            "src/_twig-components/extensions": [
                "vanpattenmedia/twig-pluralize-extension"
            ]
        }
    }
}
